## Dockerで利用する環境変数 ##
export LARAVEL_VERSION='9.*'
export PROJECT_ROOT=$(pwd)
export PROJECT_NAME=smalog
export APP_NAME=smalog
export DB_DATABASE=smalog_db
export DB_USER=user
export DB_PASSWORD=password
export USER=user
export PROXY_PORT=8090
export DB_PORT=43306
export PHP_MYADMIN_PORT=8091
# 128Mの形式で指定する
export MEMORY_LIMIT=128M
export UPLOAD_LIMIT=64M

## ↓現時点では利用用途なし
export BACKEND_PORT=
export FRONTEND_PORT=

## gitセットアップスクリプトで利用する環境変数 ##
# レポジトリ
export GIT_REPOSITORY=
# トークン
export GIT_TOKEN=
# ユーザー名
export GIT_USER=
